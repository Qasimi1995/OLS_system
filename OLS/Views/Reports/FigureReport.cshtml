@model IList<OLS.Models.FigureReport>

@{
    ViewData["Title"] = "GetTeachersList";
}

<div class="card">
    <div class="card-body">
        <h4 class="card-title">ارقام </h4>
        <div class="table-responsive mt-4">
            <table id="example23"
                   class="display nowrap table table-hover table-striped table-bordered"
                   cellspacing="0" width="100%">
                <thead>
                    <tr>
                        <th>شماره</th>
                        <th>ولایت</th>
                        <th>تعدادجواز توزیع شده</th>
                        <th>تحت کار</th>
                     
                    </tr>
                </thead>
                <tbody>
                    @for (int i = 0; i < Model.Count(); i++)
                    {

                        <tr>
                            <td>
                                @(i + 1)
                            </td>

                            <td>
                                @Html.DisplayFor(modelItem => Model[i].Province)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => Model[i].LicensesIssued)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => Model[i].Underwork)
                            </td>
                        </tr>
                    }


                </tbody>
            </table>

        </div>
    </div>
</div>