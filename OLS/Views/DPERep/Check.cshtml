                            data-target="#bs-example-modal-lg">
                        Process School
                    </button>
                    <hr />
                    <!--  Modal content for the above example -->
                    <div class="modal fade" id="bs-example-modal-lg" tabindex="-1" role="dialog"
                         aria-labelledby="myLargeModalLabel" aria-hidden="true">
                        <div class="modal-dialog modal-lg">
                            <div class="modal-content">


                                <div class="modal-body">
                                    <div class="card card-outline-info">
                                        <div class="card-header">
                                            <h4 class="mb-0 text-white">Process School</h4>
                                        </div>
                                        <form asp-action="Check" method="post" id="myform2" enctype="multipart/form-data">
                                            <div class="form-body">


                                                <!--/row-->
                                                <div class="row">
                                                    <div class="col-md-6">
                                                        <div class="form-group col-md-12 mt-3">
                                                            <div class="p-5">

                                                                @if (ViewBag.ProcessStatuses != null)
                                                                {
                                                                    @foreach (var item in ViewBag.ProcessStatuses)
                                                                    {

                                                                        <input type="hidden" asp-for="ProcessProgressId" value="@item.ProcessProgressId" />
                                                                        <input type="hidden" asp-for="SubProcessId" value="@item.SubProcessId" />
                                                                        <div>
                                                                            <input type="radio" asp-for="ProcessStatusId" class="check" name="ProcessStatusId" data-radio="iradio_square-red" value="@item.ProcessStatusId">
                                                                            <span asp-validation-for="ProcessStatusId" class="text-danger"></span>
                                                                            <label>@item.StatusNameDari</label>

                                                                        </div>


                                                                    }

                                                                }

                                                            </div>

                                                        </div>

                                                    </div>
                                                    <div class="col-md-6">
                                                        <div class="form-group col-md-12 mt-3">
                                                            <label>ملاحظات</label>
                                                            <textarea asp-for="Remarks" class="form-control" rows="5"></textarea>
                                                            <span asp-validation-for="Remarks" class="text-danger"></span>
                                                        </div>

                                                    </div>


                                                    <input type="hidden" asp-for="ProcessStatusIdNav" value="@ViewBag.ProcessStatusId" />
                                                    <input type="hidden" asp-for="OrderNumber" value="@ViewBag.OrderNumber" />
                                                    <input type="hidden" asp-for="SchoolId" value="@ViewBag.SchoolInfo.SchoolId" />
                                                    <input type="hidden" asp-for="CompletionFlag" value="@ViewBag.CompletionFlag" />

                                                </div>

                                            </div>
                                            @if (ViewBag.roleid == "DB70DAE4-9422-4A2F-A14B-7552702F026B")
                                            {

                                                <input asp-for="Report" class="form-control hidden" />
                                                <span asp-validation-for="Report" class="text-danger"></span>
                                            }
                                            <div class="form-actions">
                                                <button type="submit" class="btn btn-success">
                                                    <i class="fa fa-check"></i>
                                                    Save
                                                </button>
                                                <button type="button" id="reset" data-dismiss="modal" class="btn btn-inverse">Cancel</button>
                                            </div>
                                        </form>
                                    </div>
                                </div>
                            </div><!-- /.modal-content -->
                        </div><!-- /.modal-dialog -->
