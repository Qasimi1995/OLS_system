@model OLS.ViewModels.ProcessProgressViewModel
@{
    ViewData["Title"] = "Check";

    var ext = ".pdf";
}



@if (!ViewData.ModelState.IsValid)
{
    <script>

        $(document).ready(
            function () {

                $("#bs-example-modal-lg").modal('show');
                $("#reset").click(function () {
                    $(':input', '#myform2').val("");
                });
            }

        );
    </script>

}


<div class="row">
    <div class="col-12">
        <div class="card">
